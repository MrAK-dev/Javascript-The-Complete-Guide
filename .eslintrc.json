{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "newline-after-var": [
            "error",
            "always"
        ],
        "comma-dangle": ["error", "never"],
        "import/extensions": ["never"]
    }
}
